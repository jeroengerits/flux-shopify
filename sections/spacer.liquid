<div class='spacer' style='height: {{ section.settings.height }}px;'></div>

{% schema %}
{
  "name": "Spacer",
  "settings": [
    {
      "type": "range",
      "id": "height",
      "label": "Height",
      "min": 1,
      "max": 100,
      "step": 1,
      "default": 8,
      "unit": "px"
    }
  ],
  "presets": [
    {
      "name": "Spacer"
    }
  ]
}
{% endschema %}
